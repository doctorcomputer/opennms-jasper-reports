	<queryString>
		<![CDATA[SELECT
	categories.categoryname,
	node.nodelabel,
	node.nodeid
FROM
	node
JOIN
	category_node
ON
	(category_node.nodeid = node.nodeid)
JOIN
	categories
ON
	(categories.categoryid = category_node.categoryid)
WHERE
	categories.categoryname like $P{SURVEILLANCE_CATEGORY} AND
	node.nodesysoid like '.1.3.6.1.4.1.311.%' AND
	node.nodetype = 'A'
ORDER BY
	categories.categoryname,
	node.nodelabel]]>
	</queryString>
