WHERE
    nodelabel like '$P!{NODE_LABEL}' AND
    nodetype = 'A' AND
    nodesysoid like '.1.3.6.1.4.1.311.%' AND
    foreignsource like '$P!{OCS_REQUISITION_NAME}']]>
